{
  "name": "florianajir/rabbitmq-sql-bundle",
  "type": "symfony-bundle",
  "description": "RabbitMQ messages to Sql with doctrine and php-amqplib.",
  "keywords": [
    "RabbitMQ Sql",
    "AMQP consumer"
  ],
  "homepage": "https://github.com/florianajir/rabbitmq-sql-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Florian Ajir",
      "homepage": "https://github.com/florianajir/rabbitmq-sql-bundle"
    }
  ],
  "require": {
    "php": ">=5.4.0",
    "symfony/framework-bundle": "~2.3|~3.0",
    "php-amqplib/rabbitmq-bundle": "~1.6",
    "doctrine/dbal": "^2.5",
    "jms/serializer": "^1.1"
  },
  "require-dev": {
    "phpunit/phpunit": "4.*",
    "squizlabs/php_codesniffer": "2.*",
    "sebastian/phpcpd": "2.*",
    "phpmd/phpmd": "2.*",
    "phploc/phploc": "2.*"
  },
  "autoload": {
    "psr-4": {
      "FlorianAjir\\RabbitmqSqlBundle\\": ""
    }
  },
  "support": {
    "docs": "https://github.com/florianajir/rabbitmq-sql-bundle/README.md"
  }
}
